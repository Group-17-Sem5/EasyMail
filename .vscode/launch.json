{
    "name": "Run w/ --no-null-safety",
    "type": "dart",
    "request": "launch",
    "args": [
       "--no-sound-null-safety"
    ],
    "codeLens": {
       "for": [
          "run-test",
          "run-test-file",
          "debug-test",
          "debug-test-file",
       ],
       "title": "${debugType} (--no-sound-null-safety)"
    }
 },
 
    {
        "name": "Flutter: Run all Tests",
        "type": "dart",
        "request": "launch",
        "program": "./test/"
    },
        {
            "name": "EasyMail-App_FrontEnd",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "EasyMail-App_FrontEnd (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "Flutter (test\\controller\\postManController_test.dart)",
            "type": "dart",
            "request": "launch",
            "program": "test\\controller\\postManController_test.dart",
            "args": [
                "--no-sound-null-safety"
            ]
        },
        {
            "name": "Flutter (test\\controller\\postManController_test.dart)",
            "type": "dart",
            "request": "launch",
            "program": "test\\controller\\postManController_test.dart",
            "args": [
                "--no-sound-null-safety"
            ]
        },
        {
            "name": "Flutter (test\\controller\\postManController_test.dart)",
            "type": "dart",
            "request": "launch",
